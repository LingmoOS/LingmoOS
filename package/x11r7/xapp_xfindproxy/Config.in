config LINGMO_PACKAGE_XAPP_XFINDPROXY
	bool "xfindproxy"
	select LINGMO_PACKAGE_XLIB_LIBICE
	select LINGMO_PACKAGE_XLIB_LIBXT
	select LINGMO_PACKAGE_XORGPROTO
