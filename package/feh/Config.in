config LINGMO_PACKAGE_FEH
	bool "feh"
	depends on LINGMO_PACKAGE_XORG7
	depends on LINGMO_USE_MMU # fork()
	select LINGMO_PACKAGE_XLIB_LIBXT
	select LINGMO_PACKAGE_IMLIB2
	select LINGMO_PACKAGE_IMLIB2_PNG
	select LINGMO_PACKAGE_IMLIB2_JPEG
	select LINGMO_PACKAGE_IMLIB2_X
	help
	  feh is an X11 image viewer aimed mostly at console users.

	  http://feh.finalrewind.org/
