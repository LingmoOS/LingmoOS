#: plugin_katetextfilter.cpp:238
#, kde-format
msgid "Usage: textfilter COMMAND"
msgstr "İstifadə olunur: textfilter ƏMRi"

#: plugin_katetextfilter.cpp:249
#, kde-format
msgid ""
"<qt><p>Usage: <code>textfilter COMMAND</code></p><p>Replace the selection "
"with the output of the specified shell command.</p></qt>"
msgstr ""
"<qt><p>İstifadə olunur:<code>textfilter ƏMRİ</code></p>Seçilmişi verilmiş "
"əmr üzlüyü çıxışı ilə əvəz edir.<p></p>"

#: plugin_katetextfilter.cpp:265
#, kde-format
msgid "&Filter Through Command..."
msgstr "Əmr vasitəsi ilə &süzgəcləyin..."

