#: validlocale.8:44
#, no-wrap
msgid "AUTHORS"
msgstr "AUTORES"

# type: Plain text
#: validlocale.8:47
msgid "Petter Reinholdtsen E<lt>pere@hungry.comE<gt>"
msgstr "Petter Reinholdtsen E<lt>pere@hungry.comE<gt>"

