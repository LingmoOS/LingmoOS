#, fuzzy, kde-format
#| msgid "Run in &terminal window"
msgid "Run in Terminal Window"
msgstr "Nise në &dritare terminali"

#: shellrunner.cpp:54
#, kde-format
msgid "Run %1"
msgstr "Nis %1"

#~ msgid "Enter the user you want to run the application as here."
#~ msgstr "Vendosni këtu si cili përdorues dëshironi ta përdorni programin."

#~ msgid "User&name:"
#~ msgstr "Emri i &përdoruesit:"

#~ msgid "Enter the password here for the user you specified above."
#~ msgstr ""
#~ "Vendosni fjalëkalimin këtu për përdoruesin që keni specifikuar më sipër."

#~ msgid "Pass&word:"
#~ msgstr "Fjalë^kalimi"

#~ msgid ""
#~ "Check this option if the application you want to run is a text mode "
#~ "application. The application will then be run in a terminal emulator "
#~ "window."
#~ msgstr ""
#~ "Vendosni këtë opsion nëse programi që dëshironi të nisni është një "
