[Desktop Entry]
Actions=setAsWallpaper;
Type=Service
MimeType=image/avif;image/gif;image/heif;image/jpeg;image/png;image/svg+xml;image/svg+xml-compressed;image/bmp;image/webp;image/tiff;
X-KDE-Priority=TopLevel

[Desktop Action setAsWallpaper]
Name=Set as Login Screen
Name[x-test]=xxSet as Login Screenxx
Name[zh_CN]=设为登录界面背景
Icon=viewimage
Exec=pkexec cp "%u" /etc/system/wallpaper/img.jpg