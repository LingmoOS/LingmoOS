# Code coverage support

.PHONY: coverage coverage-clean

@COVERAGE_ENABLED_TRUE@coverage: all
@COVERAGE_ENABLED_TRUE@	: # Remove coverage data from any previous run
@COVERAGE_ENABLED_TRUE@	rm -f *.lcov
@COVERAGE_ENABLED_TRUE@	find -name '*.gcda' -o -name '*.gcov' | xargs rm -f

@COVERAGE_ENABLED_TRUE@	: # Initialize data
@COVERAGE_ENABLED_TRUE@	$(LCOV) $(LCOV_CAPTURE_OPTS) --capture --initial \
@COVERAGE_ENABLED_TRUE@	  --output-file dpkg_base.lcov
@COVERAGE_ENABLED_TRUE@	: # Run test cases
@COVERAGE_ENABLED_TRUE@	$(MAKE) -C lib/dpkg check
@COVERAGE_ENABLED_TRUE@	$(MAKE) -C src check
@COVERAGE_ENABLED_TRUE@	$(MAKE) -C utils check
@COVERAGE_ENABLED_TRUE@	: # Merge test coverage data
@COVERAGE_ENABLED_TRUE@	$(LCOV) $(LCOV_CAPTURE_OPTS) --capture \
@COVERAGE_ENABLED_TRUE@	  --output-file dpkg_test.lcov
@COVERAGE_ENABLED_TRUE@	$(LCOV) $(LCOV_OPTS) \
@COVERAGE_ENABLED_TRUE@	  --add-tracefile dpkg_base.lcov \
@COVERAGE_ENABLED_TRUE@	  --add-tracefile dpkg_test.lcov \
@COVERAGE_ENABLED_TRUE@	  --output-file dpkg.lcov
@COVERAGE_ENABLED_TRUE@	: # Generate reports
@COVERAGE_ENABLED_TRUE@	$(LCOV) $(LCOV_OPTS) --list dpkg.lcov
@COVERAGE_ENABLED_TRUE@	$(LCOV_GENHTML) $(LCOV_OPTS) \
@COVERAGE_ENABLED_TRUE@	  --legend --title "dpkg $(VERSION) C code coverage" \
@COVERAGE_ENABLED_TRUE@	  --output-directory doc/coverage dpkg.lcov

@COVERAGE_ENABLED_TRUE@	$(MAKE) -C scripts $@

@COVERAGE_ENABLED_TRUE@	: # XXX: Inject perl coverage into lcov index files. This is a fragile
@COVERAGE_ENABLED_TRUE@	: # hack which might break depending on the html output generated.
@COVERAGE_ENABLED_TRUE@	$(LCOV_INJECT) doc/coverage/index-sort-b.html
@COVERAGE_ENABLED_TRUE@	$(LCOV_INJECT) doc/coverage/index-sort-f.html
@COVERAGE_ENABLED_TRUE@	$(LCOV_INJECT) doc/coverage/index-sort-l.html
@COVERAGE_ENABLED_TRUE@	$(LCOV_INJECT) doc/coverage/index.html

@COVERAGE_ENABLED_TRUE@coverage-clean:
@COVERAGE_ENABLED_TRUE@	rm -rf doc/coverage/
@COVERAGE_ENABLED_TRUE@	find -name '*.gcno' -o -name '*.gcda' -o \
@COVERAGE_ENABLED_TRUE@	     -name '*.gcov' -o -name '*.lcov' | xargs rm -f
@COVERAGE_ENABLED_TRUE@	$(MAKE) -C scripts $@
@COVERAGE_ENABLED_FALSE@coverage:
@COVERAGE_ENABLED_FALSE@	@echo "Need to reconfigure with --enable-coverage"

@COVERAGE_ENABLED_FALSE@coverage-clean:

