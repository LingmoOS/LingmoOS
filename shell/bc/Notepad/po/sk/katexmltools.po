#: plugin_katexmltools.cpp:447
#, kde-format
msgid ""
"The current file has been identified as a document of type \"%1\". The meta "
"DTD for this document type will now be loaded."
msgstr ""
"Aktuálny súbor bol identifikovaný ako typu \"%1\". Načíta sa meta DTD pre "
"tento typ dokumentu."

#: plugin_katexmltools.cpp:451
#, kde-format
msgid "Loading XML Meta DTD"
msgstr "Načítavam XML Meta DTD"

#: plugin_katexmltools.cpp:483
#, kde-format
msgid "The file '%1' could not be opened. The server returned an error."
msgstr "Súbor '%1' nie je možné otvoriť. Server vrátil chybu."

#: plugin_katexmltools.cpp:486 pseudo_dtd.cpp:48 pseudo_dtd.cpp:60
#, kde-format
msgid "XML Plugin Error"
msgstr "Chyba modulu XML"

#: plugin_katexmltools.cpp:991
#, kde-format
msgid "XML entities"
msgstr "XML entity"

#: plugin_katexmltools.cpp:993
#, kde-format
msgid "XML attribute values"
msgstr "Hodnoty entít XML"

#: plugin_katexmltools.cpp:995
#, kde-format
msgid "XML attributes"
msgstr "XML atribúty"

#: plugin_katexmltools.cpp:998
#, kde-format
msgid "XML elements"
msgstr "XML prvky"

#: plugin_katexmltools.cpp:1038
#, kde-format
msgid "Insert XML Element"
msgstr "Vložiť XML element"

#: plugin_katexmltools.cpp:1043
#, kde-format
msgid ""
"Enter XML tag name and attributes (\"<\", \">\" and closing tag will be "
"supplied):"
msgstr ""
"Zadajte meno a atribúty značky XML. (Znaky \"<\", \">\" a zatvárací element "
"budú doplnené):"

#: pseudo_dtd.cpp:45
#, kde-format
msgid ""
"The file '%1' could not be parsed. Please check that the file is well-formed "
"XML."
msgstr ""
"Súbor '%1' nie je možné spracovať. Prosím, overte, že sa jedná o platný "
"dokument XML."

#: pseudo_dtd.cpp:54
#, kde-format
msgid ""
"The file '%1' is not in the expected format. Please check that the file is "
"of this type:\n"
"-//Norman Walsh//DTD DTDParse V2.0//EN\n"
"You can produce such files with dtdparse. See the Kate Plugin documentation "
"for more information."
msgstr ""
"Súbor '%1' nie je v očakávanom formáte. Prosím, overte, že je to platný "
"súbor tohto typu:\n"
"-//Norman Walsh//DTD DTDParse V2.0//EN\n"
"Takéto súbory môžete vytvoriť pomocou dtdparse. Ďalšie informácie nájdete v "
"dokumentácii modulu Kate."

#: pseudo_dtd.cpp:70
#, kde-format
msgid "Analyzing meta DTD..."
msgstr "Analyzujem meta DTD..."

#: pseudo_dtd.cpp:70
#, kde-format
msgid "Cancel"
msgstr "Zrušiť"

