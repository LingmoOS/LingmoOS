#: plugin_katexmlcheck.cpp:307
#, kde-format
msgid ""
"<b>Error:</b> Failed to find xmllint. Please make sure that xmllint is "
"installed. It is part of libxml2."
msgstr ""
"<b>Errorea:</b> «xmllint» aurkitzea huts egin du. Mesedez, ziurtatu "
"«xmllint» instalatuta dagoela. «libxml2»ren zati da."

#: plugin_katexmlcheck.cpp:394
#, kde-format
msgid ""
"<b>Error:</b> Failed to execute xmllint. Please make sure that xmllint is "
"installed. It is part of libxml2."
msgstr ""
"<b>Errorea:</b> Huts egin du «xmllint» exekutatzea. Ziurtatu «xmllint» "
"instalatua dagoela. «libxml2»ren zati da."

