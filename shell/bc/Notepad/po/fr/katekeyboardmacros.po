#, kde-format
msgid "Keyboard Macros"
msgstr "Macros de clavier"

#: keyboardmacrosplugin.cpp:143
#, kde-format
msgid "<b>Keyboard Macros:</b> %1"
msgstr "<b>Macros de clavier : </b> %1"

#: keyboardmacrosplugin.cpp:242
#, kde-format
msgid "Recording…"
msgstr "Enregistrement..."

#: keyboardmacrosplugin.cpp:278
#, kde-format
msgid "ended"
msgstr "terminé"

#: keyboardmacrosplugin.cpp:278
#, kde-format
msgid "canceled"
msgstr "annulé"

#: keyboardmacrosplugin.cpp:278
#, kde-format
msgid "Recording %1"
msgstr "Enregistrement de %1..."

#: keyboardmacrosplugin.cpp:321
#, kde-format
msgid "Saved '%1'"
msgstr "« %1 » enregistré"

#: keyboardmacrosplugin.cpp:340
#, kde-format
msgid "Loaded '%1'"
msgstr "« %1 » chargé"

#: keyboardmacrosplugin.cpp:357
