<!DOCTYPE node PUBLIC "-//freedesktop//DTD D-BUS Object Introspection 1.0//EN"
         "http://www.freedesktop.org/standards/dbus/1.0/introspect.dtd">
<node name="/com/lingmo/ProcessManager">
    <interface name="com.lingmo.ProcessManager.AppWhitelist">
        <method name="AddApp">
            <arg name="desktopFile" type="s" direction="in"></arg>
            <arg name="succeeded" type="b" direction="out"></arg>
        </method>
        <method name="RemoveApp">
            <arg name="desktopFile" type="s" direction="in"></arg>
            <arg name="succeeded" type="b" direction="out"></arg>
        </method>
        <method name="AppList">
            <arg name="appList" type="as" direction="out"></arg>
        </method>
    </interface>
</node>
