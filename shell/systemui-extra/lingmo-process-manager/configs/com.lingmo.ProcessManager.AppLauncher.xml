<!DOCTYPE node PUBLIC "-//freedesktop//DTD D-BUS Object Introspection 1.0//EN"
         "http://www.freedesktop.org/standards/dbus/1.0/introspect.dtd">
<node name="/com/lingmo/ProcessManager">
    <interface name="com.lingmo.ProcessManager.AppLauncher">
        <method name="LaunchApp">
            <arg name="desktopFile" type="s" direction="in"></arg>
        </method>
        <method name="LaunchAppWithArguments">
            <arg name="desktopFile" type="s" direction="in"></arg>
            <arg name="args" type="as" direction="in"></arg>
        </method>
        <method name="LaunchDefaultAppWithUrl">
            <arg name="url" type="s" direction="in"></arg>
        </method>
        <method name="RunCommand">
            <arg name="command" type="s" direction="in"></arg>
        </method>
        <method name="GetAvailableAppListForFile">
            <arg name="fileName" type="s" direction="in"></arg>
            <arg name="desktopFiles" type="as" direction="out"></arg>
        </method>
        <method name="GetDesktopFileByPid">
            <arg name="pid" type="i" direction="in"></arg>
            <arg name="desktopFile" type="s" direction="out"></arg>
        </method>
        <signal name="AppLaunched">
            <arg name="desktopFile" type="s" direction="out"></arg>
        </signal>
    </interface>
</node>
