#!/bin/bash
  
set -e

path="/usr/lib/`/usr/bin/dpkg-architecture -qDEB_TARGET_MULTIARCH`/lingmo-control-center/libnetconnect.so"
dpkg-divert --package lingmo-nm  --rename --divert "$path"".old" --add $path

path2="/usr/lib/`/usr/bin/dpkg-architecture -qDEB_TARGET_MULTIARCH`/lingmo-control-center/libproxy.so"
dpkg-divert --package lingmo-nm  --rename --divert "$path2"".old" --add $path2

path3="/usr/lib/`/usr/bin/dpkg-architecture -qDEB_TARGET_MULTIARCH`/lingmo-control-center/libvpn.so"
dpkg-divert --package lingmo-nm  --rename --divert "$path3"".old" --add $path3
