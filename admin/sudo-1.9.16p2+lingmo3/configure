  *) :

     as_fn_append LT_LDFLAGS " -no-undefined"
     { { printf "%s\n" "$as_me:${as_lineno-$LINENO}: : LT_LDFLAGS=\"\$LT_LDFLAGS\""; } >&5
  (: LT_LDFLAGS="$LT_LDFLAGS") 2>&5
  ac_status=$?
  printf "%s\n" "$as_me:${as_lineno-$LINENO}: \$? = $ac_status" >&5
